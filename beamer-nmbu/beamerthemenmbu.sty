% Beamer theme NMBU
%
%
% Based on: Beamer-NMBU by Eivysaur
\mode<presentation>

% Must have packages
\RequirePackage{tikz}
\usetikzlibrary{backgrounds}
\usetikzlibrary{positioning}

% Specify theme files
\useinnertheme{nmbu}
\useoutertheme{nmbu}
\usecolortheme{nmbu}
\usefonttheme{nmbu}

% Remove navigation symbols
\setbeamertemplate{navigation symbols}{}
\setbeamertemplate{caption}[numbered]

\mode<all>